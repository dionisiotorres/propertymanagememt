<odoo>
  <data>
    <record model="ir.ui.view" id="view_utility_supply_type_form">
      <field name="name">Utility Supply Type Form</field>
      <field name="model">pms.utility.supply.type</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
           <div class="oe_button_box" name="button_box">
              <button type="object" class="oe_stat_button" icon="fa-archive">
                  <!-- <field name="active" widget="boolean_button" options='{"terminology": "archive"}'/> -->
              </button>
            </div>
            <div class="oe_title">
            <label for="name" string="Description"/>
            <h1>
                <field name="name" placeholder="Description"/>
            </h1>
          </div>
          <group>
            <group>
                <field name="property_id"/>
                <field name="code" placeholder="Code"/>
            </group>
          </group>
          </sheet>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="view_utility_supply_type_tree">
      <field name="name">Utility Supply Type list</field>
      <field name="model">pms.utility.supply.type</field>
      <field name="arch" type="xml">
        <tree>
          <field name="code"/>
          <field name="name"/>
        </tree>
      </field>
    </record>
     <record model="ir.ui.view" id="view_utility_supply_type_filter">
      <field name="name">Utility Supply Type list</field>
      <field name="model">pms.utility.supply.type</field>
      <field name="arch" type="xml">
        <search>
          <field name="code"/>
          <field name="name"/>
        </search>
      </field>
    </record>
     <record id="action_utility_supply_type_all" model="ir.actions.act_window">
       <field name="name">Utility Supply Types</field>
       <field name="res_model">pms.utility.supply.type</field>
       <field name="view_type">form</field>
       <field name="view_mode">tree,form</field>
       <field name="view_id" ref="view_utility_supply_type_tree"/> 
    </record>
    <odoo>
  <data>
    <record model="ir.ui.view" id="view_pms_utility_source_type_form">
      <field name="name">Utility Source Type Form</field>
      <field name="model">pms.utility.source.type</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <div class="oe_button_box" name="button_box">
              <button type="object" class="oe_stat_button" icon="fa-archive">
                  <!-- <field name="active" widget="boolean_button" options='{"terminology": "archive"}'/> -->
              </button>
            </div>
            <div class="oe_title">
            <label for="name" string="Description"/>
            <h1>
                <field name="name" placeholder="Description"/>
            </h1>
          </div>
          <group>
            <group>
                <field name="property_id"/>
                <field name="code" placeholder="Code"/>
                <field name="utility_type_id" placeholder="GEM"/>
            </group>
          </group>
          </sheet>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="view_pms_utility_source_type_tree">
      <field name="name">Utility Source Type list</field>
      <field name="model">pms.utility.source.type</field>
      <field name="arch" type="xml">
        <tree>
            <field name="code"/>
            <field name="name"/>
            <field name="utility_type_id"/>
        </tree>
      </field>
    </record>
     <record model="ir.ui.view" id="view_pms_utility_source_type_filter">
      <field name="name">Utility Source Type list</field>
      <field name="model">pms.utility.source.type</field>
      <field name="arch" type="xml">
        <search>
            <field name="code"/>
            <field name="name"/>
            <field name="utility_type_id"/>
        </search>
      </field>
    </record>
    <record id="action_utility_source_type_all" model="ir.actions.act_window">
       <field name="name">Utility Source Types</field>
       <field name="res_model">pms.utility.source.type</field>
       <field name="view_type">form</field>
       <field name="view_mode">tree,form</field>
       <field name="view_id" ref="view_pms_utility_source_type_tree"/> 
    </record>
  </data>
</odoo>
    <menuitem  id="utility_type_menu_root" 
        name="Utility Supply Type" 
        parent="property_management_menu_sub"
        action="action_utility_supply_type_all"/>
    <menuitem  id="utility_source_type_menu_root" 
        name="Utility Source Type" 
        parent="property_management_menu_sub"
        action="action_utility_source_type_all"/>
  </data> 
</odoo>